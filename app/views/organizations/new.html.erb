<div id="post_content">
  <h1>Create new Organization</h1>

  <%= form_for :organization, url: {action: "create"} do |f| %>
    
    <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </p>

    <p>
      <%= f.label :phone %><br>
      <%= f.text_field :phone %>
    </p>

    <p>
      <%= f.label :email %><br>
      <%= f.text_field :email %>
    </p>

    <p>
      <%= f.label :website %><br>
      <%= f.text_field :website %>
    </p>

    <p>
      <%= f.label :logo %><br>
      <%= f.file_field :logo %>
    </p>

    <p>
    <%= f.submit %>
    </p>

  <% end %>
</div>

