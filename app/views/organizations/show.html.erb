<div id="post_content">
  <p class ="title">
    Name: <%= @organization.name %>
  </p>

  <p class="date">
    <%= @organization.created_at %>
    <% if user_signed_in? && current_user.user_type == "account_manager" %>
      | <%= link_to "Edit", edit_organization_path(@organization) %> |
      <%= link_to "Delete", organization_path(@organization), method: :delete, data: { confirm: 'Are you sure ?' } %>
    <% end %>
  </p>

  <p class="body">
    Phone: <%= @organization.phone %>
  </p>

  <p class="body">
    Email: <%= @organization.email %>
  </p>

  <p class="body">
    Website: <%= @organization.website %>
  </p>

  <div class="Body">
    Logo:
    <%= image_tag(@organization.logo) %>
  </div>
</div>

